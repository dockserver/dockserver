{
  "categories": [
    {
      "title": "## ğŸ”£ Features",
      "labels": ["feature"]
    },
    {
      "title": "## ğŸ› Fixes",
      "labels": ["fix"]
    },
    {
      "title": "## ğŸ§ª Tests",
      "labels": ["test"]
    },
    {
      "title": "## ğŸ’» PiP-Update",
      "labels": ["pip"]
    },
    {
      "title": "## ğŸš‡ Added",
      "labels": ["add"]
    },
    {
      "title": "## ğŸ’» GitHub updates",
      "labels": ["GH"]
    },
    {
      "title": "## ğŸ“¦ Composer",
      "labels": ["composer"]
    },
    {
      "title": "## ğŸ“– Documentation",
      "labels": ["docs"]
    },
    {
      "title": "## ğŸ¨ DockServer Update",
      "labels": ["Update"]
    }
  ],
  "ignore_labels": ["ignore"],
  "sort": "ASC",
  "template": "${{CHANGELOG}}\n\n\n${{UNCATEGORIZED}}\n",
  "pr_template": "- ${{TITLE}}\n   - PR: #${{NUMBER}}",
  "empty_template": "repository changes",
  "label_extractor": [
    {
      "pattern": "(.) (.+)",
      "target": "$1"
    },
    {
      "pattern": "(.) (.+)",
      "target": "$1",
      "on_property": "title"
    }
  ],
  "transformers": [
    {
      "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
      "target": "- $4\n  - $6"
    }
  ],
  "max_tags_to_fetch": 500,
  "max_pull_requests": 500,
  "max_back_track_time_days": 720,
  "exclude_merge_branches": ["Owner/qa"],
  "tag_resolver": {
    "method": "semver"
  },
  "base_branches": ["master"]
}
